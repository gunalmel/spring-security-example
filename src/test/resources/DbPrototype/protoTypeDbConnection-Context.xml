<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 	Data Source for DbUnit schema generation operation that should point to a production prototype db  -->
	<bean id="dataSourceProdDesign" class="org.springframework.jdbc.datasource.DriverManagerDataSource" >
        <property name="driverClassName" value="org.h2.Driver"/>
        <property name="url" value="jdbc:h2:src/test/resources/DbPrototype/schedulingDB;TRACE_LEVEL_FILE=1;IFEXISTS=true"/>
        <property name="username" value="sa"/>
        <property name="password" value="sa"/>
    </bean>

</beans>
